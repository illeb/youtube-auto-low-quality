{

  "name": "Youtube auto low quality",
  "version": "0.1.0",
  "description": "This extension sets your background youtube video at low quality automatically. Yes, it does just that",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ]
}